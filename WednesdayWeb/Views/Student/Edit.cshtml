@model WednesdayWeb.Models.Student
@{
    ViewBag.Title = "Edit";
}

<h2>Edit Student Info</h2>
@using (Html.BeginForm())
{
    <p>@Html.DisplayNameFor(a => a.Id) - @Html.EditorFor(model => model.Id)</p>
    <p>@Html.DisplayNameFor(model => model.Name) - @Html.EditorFor(model => model.Name)</p>
    <p>@Html.DisplayNameFor(model => model.GPA) - @Html.EditorFor(y => y.GPA)</p>
    <input type="submit" value="Save"/>
}

